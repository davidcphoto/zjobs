{
  "name": "zjobs",
  "displayName": "zJobs",
  "description": "Alternative to jobs in zowe explorer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zjobs.Open",
        "title": "Open zJobs"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "when": "view == zowe.ds.explorer",
          "command": "zjobs.Open",
          "group": "900_zjobs"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@types/vscode": "^1.101.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@zowe/imperative": "^8.23.1",
    "@zowe/zos-jobs-for-zowe-sdk": "^8.24.0",
    "@zowe/zowe-explorer-api": "^3.2.1"
  }
}
