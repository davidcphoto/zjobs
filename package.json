{
  "name": "zjobs",
  "displayName": "zJobs",
  "description": "Alternative to jobs in zowe explorer",
  "version": "1.0.1",
  "icon": "imagens/zJobs.png",
  "publisher": "DavidCanto",
  "repository": {
    "url": "https://github.com/davidcphoto/zjobs"
  },
  "keywords": [
    "jcl",
    "mainframe",
    "David Canto",
    "zos",
    "zowe",
    "job",
    "job control language",
    "jes",
    "jobs",
    "submit",
    "sd",
    "sh"
  ],
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zjobs.Open",
        "title": "zJobs - Open view"
      },
      {
        "command": "zjobs.GetJCL",
        "title": "zJobs - Get JCL"
      },
      {
        "command": "zjobs.Purge",
        "title": "zJobs - Delete job"
      },
      {
        "command": "zjobs.GetSysoutList",
        "title": "zJobs - Expand sysout list"
      },
      {
        "command": "zjobs.Refresh",
        "title": "zJobs - Refresh"
      },
      {
        "command": "zjobs.Cancel",
        "title": "zJobs - Cancel"
      },
      {
        "command": "zjobs.GetSysoutList",
        "title": "zJobs - Expand sysout list"
      },
      {
        "command": "zjobs.GetSysout",
        "title": "zJobs - Get Sysout file"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "when": "(view == zowe.ds.explorer || view == zowe.jobs.explorer)",
          "command": "zjobs.Open",
          "group": "900_zjobs"
        }
      ],
      "webview/context": [
        {
          "command": "zjobs.Refresh",
          "group": "zjobs@01",
          "when": "webviewId == 'zJobsWebview' && (webviewSection == 'Job' || webviewSection == 'Spool')"
        },
        {
          "command": "zjobs.GetJCL",
          "group": "zjobs@02",
          "when": "webviewId == 'zJobsWebview' && (webviewSection == 'Job' || webviewSection == 'Spool')"
        },
        {
          "command": "zjobs.Cancel",
          "group": "zjobs@03",
          "when": "webviewId == 'zJobsWebview' && (webviewSection == 'Job' || webviewSection == 'Spool')"
        },
        {
          "command": "zjobs.Purge",
          "group": "zjobs@04",
          "when": "webviewId == 'zJobsWebview' && (webviewSection == 'Job' || webviewSection == 'Spool')"
        },
        {
          "command": "zjobs.GetSysoutList",
          "group": "zjobs@05",
          "when": "webviewId == 'zJobsWebview' && webviewSection == 'Job'"
        },
        {
          "command": "zjobs.GetSysout",
          "group": "zjobs@06",
          "when": "webviewId == 'zJobsWebview' && webviewSection == 'Spool'"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "^24.6.2",
    "@types/vscode": "^1.104.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^9.36.0",
    "glob": "^11.0.3",
    "mocha": "^11.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@zowe/zos-jobs-for-zowe-sdk": "^8.27.1",
    "@zowe/zowe-explorer-api": "^3.3.0"
  }
}
